<div class="page-header">
  <%= link_to orderes_order_products_path(@order), class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Voltar
  <% end %>
  <%= link_to edit_orderes_order_product_path(@order, @order_product), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h1>Mostrar</h1>
</div>

<dl class="dl-horizontal">
  <dt>Orderes:</dt>
  <dd><%= @order_product.orderes %></dd>

  <dt>Product:</dt>
  <dd><%= @order_product.product %></dd>

  <dt>Value total:</dt>
  <dd><%= @order_product.value_total %></dd>

</dl>
